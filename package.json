{
  "name": "sapegin",
  "private": true,
  "license": "See license in <Readme.md>",
  "scripts": {
    "start": "gatsby develop",
    "pretest": "npm run lint",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "typecheck": "tsc",
    "build": "gatsby build",
    "lint": "eslint . --ext .js,.tsx --cache --fix",
    "format": "prettier --loglevel warn --write \"**/*.{js,md}\"",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "react": "^16.14",
    "react-dom": "^16.14",
    "react-group": "^3.0.2",
    "react-helmet": "^6.1.0",
    "sort-on": "^4.1.0",
    "styled-components": "^5.1.1",
    "tamia": "6.0.10",
    "tamia-gatsby-link": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^16.3.1",
    "@types/react": "^16",
    "@types/react-dom": "^16",
    "@types/react-helmet": "^6.1.2",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-tamia": "^7.2.6",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "gatsby": "3.15.0",
    "gatsby-plugin-goatcounter": "0.4.0",
    "gatsby-plugin-netlify": "3.9.0",
    "gatsby-plugin-react-helmet": "4.9.0",
    "gatsby-plugin-styled-components": "4.9.0",
    "gatsby-plugin-typescript": "3.9.0",
    "gatsby-source-filesystem": "3.9.0",
    "gatsby-transformer-remark": "4.6.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "react-styleguidist": "^11.0.8",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,tsx,md}": "prettier --write",
    "*.{js,tsx}": "eslint --cache --fix"
  }
}
