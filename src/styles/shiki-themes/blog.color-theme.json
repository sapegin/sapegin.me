        "source.coffee",
        "source.yaml",
        "source.json",
        "source.diff"
      ],
      "settings": {
        "foreground": "var(--colors-code-gray)"
      }
    },
    {
      "scope": [
        "variable",
        "markup.inserted.diff",
        "entity.name.class",
        "entity.name.type.class",
        "support.type"
