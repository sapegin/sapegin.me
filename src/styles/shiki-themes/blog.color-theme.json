{
  "name": "Squirrelsong Light",
  "type": "light",
  "colors": {
    "editor.foreground": "inherit",
    "editor.background": "transparent"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "invalid.deprecated",
        "token.debug-token",
        "keyword.operator.class",
        "constant.other",
        "source.php.embedded.line",
        "support.other.variable",
        "constant.other.symbol",
